{
  "name": "@tradetrust-tt/w3c-issuer",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cjs/w3c-issuer/src/index.js",
  "module": "dist/esm/w3c-issuer/src/index.js",
  "types": "dist/types/w3c-issuer/src/index.d.ts",
  "files": [
    "dist"
  ],
  "nx": {
    "implicitDependencies": [
      "@tradetrust-tt/w3c-utils"
    ],
    "tags": [
      "type:package"
    ]
  },
  "scripts": {
    "test": "npx vitest --run",
    "lint": "npx eslint . --color --format=table --max-warnings=0",
    "build": "nx run @tradetrust-tt/w3c-issuer:'clean' && nx run @tradetrust-tt/w3c-issuer:'build:cjs' && nx run @tradetrust-tt/w3c-issuer:'build:esm' && nx run @tradetrust-tt/w3c-issuer:'build:type'",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && tscpaths -p tsconfig.build.cjs.json -s ./dist/cjs/w3c-issuer/src -o ./dist/cjs/w3c-issuer/src",
    "build:esm": "tsc --module es2020 --outDir dist/esm && tscpaths -p tsconfig.build.esm.json -s ./dist/esm/w3c-issuer/src -o ./dist/esm/w3c-issuer/src",
    "build:type": "tsc -d --emitDeclarationOnly --outDir dist/types",
    "clean": "rm -rf dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "18.x"
  },
  "exports": {
    ".": {
      "require": "./dist/cjs/w3c-issuer/src/index.js",
      "import": "./dist/esm/w3c-issuer/src/index.js",
      "types": "./dist/types/w3c-issuer/src/index.d.ts"
    },
    "./did-web": {
      "require": "./dist/cjs/w3c-issuer/src/did-web/*",
      "import": "./dist/esm/w3c-issuer/src/did-web/*",
      "types": "./dist/types/w3c-issuer/src/did-web/*"
    },
    "./*": {
      "require": "./dist/cjs/w3c-issuer/src/*.js",
      "import": "./dist/esm/w3c-issuer/src/*.js",
      "types": "./dist/types/w3c-issuer/src/*.d.ts"
    }
  },
  "dependencies": {
    "@transmute/did-key-bls12381": "^0.3.0-unstable.10",
    "@transmute/did-key-ed25519": "^0.3.0-unstable.10",
    "did-resolver": "^4.1.0",
    "jsonld-signatures": "^11.2.1",
    "multiformats": "^9.9.0",
    "web-did-resolver": "^2.0.27"
  }
}
